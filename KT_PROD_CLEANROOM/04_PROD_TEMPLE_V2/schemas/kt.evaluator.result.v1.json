{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "KT Evaluator Result v1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_id",
    "schema_version_hash",
    "adapter_id",
    "adapter_version",
    "battery_id",
    "results",
    "final_verdict"
  ],
  "properties": {
    "schema_id": {
      "const": "kt.evaluator.result.v1"
    },
    "schema_version_hash": {
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    },
    "adapter_id": {
      "type": "string"
    },
    "adapter_version": {
      "type": "string"
    },
    "battery_id": {
      "type": "string"
    },
    "results": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "final_verdict": {
      "type": "string",
      "enum": [
        "PASS",
        "FAIL"
      ]
    }
  }
}
