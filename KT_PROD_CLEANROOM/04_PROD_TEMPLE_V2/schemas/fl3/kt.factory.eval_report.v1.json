{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "KT FL3 Factory Eval Report v1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_id",
    "schema_version_hash",
    "eval_id",
    "job_id",
    "adapter_id",
    "adapter_version",
    "battery_id",
    "results",
    "final_verdict",
    "created_at"
  ],
  "properties": {
    "schema_id": { "const": "kt.factory.eval_report.v1" },
    "schema_version_hash": { "type": "string", "pattern": "^[0-9a-f]{64}$" },
    "eval_id": { "type": "string", "pattern": "^[0-9a-f]{64}$" },
    "job_id": { "type": "string", "pattern": "^[0-9a-f]{64}$" },
    "adapter_id": { "type": "string" },
    "adapter_version": { "type": "string" },
    "battery_id": { "type": "string" },
    "results": { "type": "object" },
    "final_verdict": { "type": "string", "enum": [ "PASS", "FAIL" ] },
    "created_at": { "type": "string" }
  }
}

